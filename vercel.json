{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install && npm run install:frontend",
  "devCommand": "npm start",
  "framework": null,
  "build": {
    "env": {
      "CI": "false"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": false
  }
}
